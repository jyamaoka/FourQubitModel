Language: Cpp
BasedOnStyle: Google
AccessModifierOffset: -2 # access modifiers same indent as class
#PenaltyBreakAssignment: 40 # keep assignment on same line
IncludeBlocks:   Regroup
IncludeCategories:
  - Regex:           '^<ext/.*\.h>'
    Priority:        2
    SortPriority:    0
    CaseSensitive:   false
  - Regex:           '^<.*\.h>'
    Priority:        1
    SortPriority:    0
    CaseSensitive:   false
  - Regex:           '^<.*'
    Priority:        2
    SortPriority:    0
    CaseSensitive:   false
  - Regex:           '.*'
    Priority:        3
    SortPriority:    0
    CaseSensitive:   false
  - Regex:           'G4*'
    Priority:        1
    SortPriority:    0
    CaseSensitive:   true
  - Regex:           'G4CMP*'
    Priority:        1
    SortPriority:    1
    CaseSensitive:   true